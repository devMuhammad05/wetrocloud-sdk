{
    "name": "wetrocloud/wetrocloud-sdk",
    "description": "An easy-to-use SDK that enables integration with the WetroCloud API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "wetrocloud",
        "php",
        "sdk",
        "wetrocloud sdk",
        "wetrocloud api"
    ],
    "autoload": {
        "psr-4": {
            "Wetrocloud\\WetrocloudSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Muhammad Yahaya"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "7.9"
    },
    "require-dev": {
        "larastan/larastan": "3.0",
        "pestphp/pest": "4.x-dev",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
